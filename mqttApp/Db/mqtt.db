record(ai, "$(P)$(R)ai"){
  field(DESC, "Analog Input Record")
  field(DTYP, "asynInt32")
  field(INP, "@asyn($(PORT)) FLAT:INT test/analogtopic)")
}

record(ao, "$(P)$(R)ao"){
  field(DESC, "Analog Out Record")
  field(DTYP, "asynInt32")
  field(OUT, "@asyn($(PORT)) FLAT:INT test/analogtopic)")
}

